#!/bin/bash

#
## Gnome-Bluetooth pin-code-database Default Settings
#

SRC_FILE=/usr/share/debian-system-adjustments/gnome-control-center/org.freedesktop.Tracker.Miner.Files.gschema.xml
TARGET_FILE=/usr/share/glib-2.0/schemas/org.freedesktop.Tracker.Miner.Files.gschema.xml

if [[ -e $SRC_FILE ]] && [[ -e $TARGET_FILE ]]; then
  cp $SRC_FILE $TARGET_FILE
  glib-compile-schemas /usr/share/glib-2.0/schemas/
fi
